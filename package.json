{
  "name": "gittimer",
  "version": "1.0.0",
  "description": "Electron program that will count time spend on specific branch",
  "main": "dist/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasvy/gittimer.git"
  },
  "keywords": [
    "git",
    "timer",
    "time",
    "measure",
    "github",
    "branch"
  ],
  "scripts": {
    "test": "set NODE_ENV=test && webpack --config ./webpack.test.config.js && mocha ./dist/test.js --exit",
    "compile": "webpack --config ./webpack.config.js",
    "start": "set NODE_ENV=development && npm run compile && electron ./dist/electron.js",
    "build": "set NODE_ENV=production && npm run compile && electron-builder "
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-jest": "^25.1.0",
    "babel-plugin-root-import": "^6.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^3.4.2",
    "electron": "^8.0.2",
    "electron-builder": "^22.4.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^7.1.1",
    "mocha-webpack": "^1.1.0",
    "mock-require": "^3.0.3",
    "mockery": "^2.1.0",
    "node-sass": "^4.13.1",
    "rewire-webpack": "^1.0.1",
    "rewiremock": "^3.13.9",
    "sass-loader": "^7.3.1",
    "sinon": "^9.0.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "asmcrypto-lite": "^1.1.0",
    "babel-loader": "^8.0.6",
    "electron-positioner": "^4.1.0",
    "electron-store": "^5.1.1",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-vue": "^0.10.1",
    "shebang-loader": "0.0.1",
    "simple-git": "^1.132.0",
    "style-loader": "^1.1.3",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.1.6",
    "vue2-transitions": "^0.3.0"
  },
  "author": "Lukas Vyslocky",
  "license": "MIT",
  "build": {
    "appId": "com.lukasvy.gittimer",
    "productName": "GitTimer",
    "mac": {
      "icon": "/public/icons/git-brach.icns",
      "category": "public.utilities"
    },
    "win": {
      "target": "portable",
      "icon": "/public/icons/git-branch.ico"
    },
    "portable": {
      "unicode": false,
      "artifactName": "GitTimer.exe"
    },
    "files": [
      "./public/**/*",
      "./dist/**/*",
      "!**/node_modules/*",
      "!package.json"
    ],
    "directories": {
      "output": "build"
    }
  },
  "bugs": {
    "url": "https://github.com/lukasvy/gittimer/issues"
  },
  "homepage": "https://github.com/lukasvy/gittimer#readme"
}
